//require_once (BINPATH . 'ConcreteMemento.php');
require_once (BINPATH . 'Ihm.php');
require_once (BINPATH . 'Buffer.php');
${definition}



/**/
function &getInstance()
{
	$c = \"Session\";//get_class($o);
	if ( !isset($_SESSION[ $c ]) ) 
	{
		$o =& Session::getInstance();
		$_SESSION[ $c ] =& $o;
	}
	return $_SESSION[ $c ];
}

?>
"
      associated_classes
	class_ref 129538 // Session
      end
    end

